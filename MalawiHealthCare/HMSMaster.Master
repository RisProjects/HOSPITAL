<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="HMSMaster.master.cs" Inherits="HMSMalawi.HMSMaster" %>

<!DOCTYPE html>

<html>
<head runat="server">
    
    
    <title></title>
    
      <style type="text/css">
        body {
            font-family: Arial;
